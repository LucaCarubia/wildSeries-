{% extends 'base.html.twig' %}

{% block body %}

    <h3>Saison {{ season.id }} de {{ program.title }}</h3>
    <p>{{ program.summary }}</p><br>
    <ul>
        {% for episode in episode %}
            <li><strong>Episode {{ episode.id }} :</strong>  {{ episode.title }}</li>
        {% endfor %}
    </ul>
{% endblock %}